---
import Request, { HttpMethod } from "../components/ui/Request.astro";
import { profileResponse } from "../data/users";
import Layout from "../layouts/Layout.astro";
---

<Layout>
  <h2 class="text-3xl mt-20">Users</h2>
  <div class="space-y-20 mt-10">
    <Request
      method={HttpMethod.GET}
      url="/api/users/me"
      previewJSON={null}
      Response={profileResponse}
      credentialsInclude
    />
  </div>
</Layout>
